{
    "event": "NEW_PROCESS",
    "rules": [
        { 
            "op": "is windows"
        }, 
        {
            "path": "event/FILE_PATH", 
            "case sensitive": false, 
            "re": ".*(?:(?:\\.txt)|(?:\\.doc)|(?:\\.ppt)|(?:\\.xls)|(?:\\.zip)|(?:\\.rar)|(?:\\.rtf)|(?:\\.jpg)|(?:\\.gif)|(?:\\.pdf)|(?:\\.wmi)|(?:\\.avi)|(?: {5}.*))\\.exe", 
            "op": "matches"
        }
    ], 
    "op": "and"
}